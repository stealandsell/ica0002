$TTL	604800
gamblinggreats.io.	IN	SOA	gamblinggreats.io. ubuntu.gamblinggreats.io. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;

{% for vm in groups['dns_server'] %}
gamblinggreats.io.	IN	NS	{{ vm }}.gamblinggreats.io.
{% endfor %}

{% for vm in hostvars %}
{{ hostvars[vm]['inventory_hostname'] }}	IN	A	{{ hostvars[vm]['ansible_default_ipv4']['address'] }}
{% endfor %}
